<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link type="text/css" rel="stylesheet" href="stylesheets/materialize.min.css" media="screen,projection" />

</head>

<body>

  <div class="wrapper">
    <header>
      <h1>
        <%= title %>
      </h1>
    </header>
    <main class="main">
      <%for(var i=0; i<arr.length; i++ ) {%>
        <table class="maket">
          <tr>
            <td class="left">
              <%= arr[i]%>
            </td>
            <td class="left">
              <%= text[i]%>
            </td>
            <td>
              <label>
                <input type="checkbox" class="filled-in" />
                <span>take car</span>
              </label>
            </td>
          </tr>
        </table>
        <%}%>
          <form action="#">
            <p>
              <label>
                <input type="checkbox" />
                <span>have driver license</span>
              </label>
            </p>
          </form>
          <div class="category">
            <% for(i=0;i<4;i++){%>
              <div class="switch">
                <label>
                  <%= arr2[i]%>
                    <input type="checkbox">
                    <span class="lever"></span>
                    <%= arr3[i]%>
                </label>
              </div>
              <%}%>
                <div class="experience">
                  <span>your driving experience(from 1 to 100)</span>
                  <form action="#">
                    <p class="range-field">
                      <input type="range" id="test5" min="0" max="100" />
                    </p>
                  </form>
                </div>
          </div>
          <div class="row">
            <form class="col s12">
              <div class="row">
                <div class="input-field col s6">
                  <i class="material-icons prefix">&#x270e</i>

                  <input id="icon_prefix" type="text" class="validate">
                  <label for="icon_prefix">First Name</label>
                </div>
                <div class="input-field col s6">
                  <i class="material-icons prefix">&#9742;</i>
                  <input id="icon_telephone" type="tel" class="validate">
                  <label for="icon_telephone">Telephone</label>
                </div>
              </div>
            </form>
          </div>
          <div>
            <a class="waves-effect waves-light btn-large" href="/">accept</a>
          </div>
    </main>
    <%-include('footer.ejs')%>

  </div>
  <script type="text/javascript" src="javascripts/materialize.min.js">
  </script>
</body>


</html>